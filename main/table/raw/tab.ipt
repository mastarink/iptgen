table  begin raw
  target TRACE
  chain OUTPUT
  rule proto(tcp) not-dpts($http-ports,445) 
  cmp -A OUTPUT -m tcp -p tcp -m multiport  ! --dports http,https,445 -j TRACE
  rule proto(udp)
  cmp -A OUTPUT -m udp -p udp -j TRACE

# rule proto(icmp) icmp-type(any)
# rule proto(icmp) icmp-type(any)
  chain PREROUTING 
  rule proto(tcp) not-dpts($http-ports,445) 
  cmp -A PREROUTING -m tcp -p tcp -m multiport  ! --dports http,https,445 -j TRACE
  rule proto(udp)
  cmp -A PREROUTING -m udp -p udp -j TRACE

# rule proto(icmp) icmp-type(any)
# rule proto(icmp) icmp-type(any)
table  end raw
# x_tables: ip_tables: TRACE target: only valid in raw table, not filter

# iptables -t raw -A PREROUTING -p tcp --destination 192.168.0.0/24 --dport 80 -j TRACE
# iptables -t raw -A OUTPUT -p tcp --destination 192.168.0.0/24 --dport 80 -j TRACE


