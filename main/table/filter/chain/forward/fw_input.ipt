  log-way=from
  chain FWFinet FWFlan FWFlocal FORWARD
  
  input $ifinet   ; target FWFinet  ; rule comment(fwinputI)
  include fw_input_inet
  input $iflan    ; target FWFlan   ; rule comment(fwinputA)
  include fw_input_lan
  input $iflocal  ; target FWFlocal ; rule comment(fwinputO)
  include fw_input_local

