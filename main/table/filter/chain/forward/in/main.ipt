#
log-direction=@

input $ifinet ; output $iflan
target $chsuff

rule comment(@ forwardO)
cmp -A FORWARD -i enp2s0 -o enp4s0 -j $chsuff -m comment --comment "in forwardO"
#----------------------------------------
output - ; input - 
include acpt/main icmp/main tcp/main udp/main drop/main

chain drop$chsuff $chsuff
target drop$chsuff

rule
cmp -A Win -j dropWin


proto -
