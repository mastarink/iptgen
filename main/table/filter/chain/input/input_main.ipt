  input $ifinet   ; target Finet  ; rule comment(inputI)
  cmp -A INPUT -i enp2s0 -j Finet -m comment --comment inputI
  include input_inet
  input $iflan    ; target Flan   ; rule comment(inputA)
  cmp -A INPUT -i enp4s0 -j Flan -m comment --comment inputA
  include input_lan
  input $iflocal  ; target Flocal ; rule comment(inputO)
  cmp -A INPUT -i lo -j Flocal -m comment --comment inputO
  include input_local

