#
chain tcp$chsuff $chsuff


rule target(tcp$chsuff) proto(tcp)
cmp -A $chsuff -m tcp  -p tcp -j tcp$chsuff

chain tcp$chsuff

rule target(acpt$chsuff)
cmp -A tcp$chsuff -j acpt$chsuff
# -A tcpFlocal -i $LO_IFACE -j acptFlocal

chain drop$chsuff tcp$chsuff
rule target(drop$chsuff)
cmp -A tcp$chsuff -j drop$chsuff
# -A tcpFlocal -j dropFlocal
