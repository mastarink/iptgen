#
chain icmp$chsuff $chsuff


rule  target(icmp$chsuff) lit(--icmp-type any) proto(icmp)
cmp -A $chsuff -m icmp -p icmp  --icmp-type any -j icmp$chsuff


chain icmp$chsuff

rule target(acpt$chsuff)
cmp -A icmp$chsuff -j acpt$chsuff
# -A icmpFlocal -i $LO_IFACE -j acptFlocal


chain drop$chsuff icmp$chsuff
rule target(drop$chsuff)
cmp -A icmp$chsuff -j drop$chsuff
# -A icmpFlocal -j dropFlocal
