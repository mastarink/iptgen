#
log-direction=@

output $if@
target $chsuff

rule comment(outputI)
cmp -A OUTPUT -o enp2s0 -j 2inet -m comment --comment outputI
#----------------------------------------
chain $chsuff

output - ; input - 
include drop/main

include acpt/main icmp/main tcp/main udp/main

chain drop$chsuff $chsuff
target drop$chsuff

rule
cmp -A 2inet -j drop2inet


proto -
