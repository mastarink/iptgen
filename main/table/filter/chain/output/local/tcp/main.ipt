#
chain tcp$chsuff $chsuff


rule target(tcp$chsuff) proto(tcp)
cmp -A $chsuff -m tcp  -p tcp -j tcp$chsuff

chain tcp$chsuff

rule target(oacpt$chsuff)
cmp -A tcp$chsuff -j oacpt$chsuff
# -A tcp2local -i $LO_IFACE -j oacptFlocal

rule target(odrop$chsuff)
cmp -A tcp$chsuff -j odrop$chsuff
# -A tcp2local -j odrop2$chsuff
