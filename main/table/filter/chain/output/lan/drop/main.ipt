#
  chain odrop$chsuff $chsuff
  
  rule target(odrop$chsuff)
  cmp -A $chsuff -j odrop$chsuff

  chain odrop$chsuff

# include listed

  log-message=$log-drop-message
  rule target(LOG) $log-options
  cmp -A odrop$chsuff -j LOG --log-prefix "ipt4: NOT $log-way $log-direction" --log-uid --log-tcp-options --log-ip-options --log-level 7
  # -A odrop2local -j LOG --log-prefix "$IPT_PREF_PREF: $LOG_NOT_MARK from $chsuff " $IPTABLES_LOG_OPTS

  target DROP ; rule
  cmp -A odrop$chsuff -j DROP
